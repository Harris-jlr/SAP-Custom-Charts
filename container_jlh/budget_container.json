			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"info": {
			"type": "string",
			"description": "container info",
			"default": ""
		},
		"color": {
			"type": "string",
			"description": "Text Color info",
			"default": "#3498db"
		},
		"width": {
			"type": "integer",
			"default": 100
		},
		"height": {
			"type": "integer",
			"default": 100
		},
		"value": {
			"type": "string",
			"description": "container value",
			"default": "10"
		}
	},
	"methods": {
		"setValue": {
			"description": "Sets the container value.",
			"parameters": [
				{
					"name": "newValue",
					"type": "string",
					"description": "container value"
				},
				{
					"name": "newInfo",
					"type": "string",
					"description": "container info"
				},
				{
					"name": "newColor",
					"type": "string",
					"description": "Text Color info"
				}
			],
			"body": "this.value = newValue; this.info = newInfo; this.color = newColor;"
		},
		"getValue": {
			"returnType": "string",
			"description": "Returns the container value.",
			"body": "return this.value;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the container."
		}
	}
