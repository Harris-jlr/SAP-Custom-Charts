{
	"id": "com.test.meter",
	"version": "1.0.1",
	"name": "jlh meter",
	"description": "jlh meter",
	"newInstancePrefix": "meter",
	"icon": "test_tableau/icon.png?csf=1&web=1&e=I6mbnL",
	"vendor": "jlh",
	"eula": "EULA",
	"license": "1.0",
	"webcomponents": [
        {
			"kind": "main",
			"tag": "com-jlh-box",
			"url": "/box%20sample/box.js?csf=1&web=1&e=hfOT9l",
			"integrity": "",
			"ignoreIntegrity": true
		}
],
"properties": {
    "color": {
        "type": "string",
        "description": "Background color",
        "default": "white"
    },
    "opacity": {
        "type": "number",
        "description": "Opacity",
        "default": 1
    },
    "width": {
        "type": "integer",
        "default": 100
    },
    "height": {
        "type": "integer",
        "default": 100
    }
},
"methods": {
